@model IEnumerable<VehicleType>

<ul class="navbar-nav d-flex flex-row align-items-center" style="list-style: none; padding: 0; margin: 0;">
    @foreach (var type in Model)
    {
        <li class="nav-item me-3">
            <a asp-controller="Vehicle"
               asp-action="List"
               asp-route-type="@type.Name"
               class="nav-link px-3">
                @type.Name
            </a>
        </li>
    }

    <li class="nav-item">
        <a asp-controller="Vehicle"
           asp-action="List"
           class="nav-link px-3">
            Wszystkie
        </a>
    </li>
</ul>